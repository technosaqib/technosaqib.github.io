  <!-- WhatsApp Script -->
  <script>  
    const phone = '917889476325'; // your WhatsApp number
    function getWaLink(service){  
      return `https://wa.me/${phone}?text=${encodeURIComponent('Hi Saqib, I want to buy: ' + service + '.\nMy name: \nBusiness: \nBudget: ')}`;  
    }  
    document.querySelectorAll('.buy-btn').forEach(btn => {  
      btn.addEventListener('click', e => {  
        e.preventDefault();  
        const service = btn.getAttribute('data-service');  
        window.open(getWaLink(service), '_blank');  
      });  
    });  
    document.getElementById('waBtn').addEventListener('click', e => {  
      e.preventDefault();  
      window.open(getWaLink('General Inquiry'), '_blank');  
    });  
  </script>  
